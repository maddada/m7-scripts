                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "q",
                                    "modifiers": { "mandatory": ["command", "shift", "option"] }
                                },
                                "to": [{ "key_code": "vk_none" }],
                                "type": "basic"
                            }
                        ]
